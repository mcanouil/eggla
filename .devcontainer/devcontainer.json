{
	"name": "eggla",
	"build": {
		"dockerfile": "Dockerfile", 
		"args": {
			"TAG": "devel"
		}
	},
	// "remoteUser": "eggla",
	"features": {
		"ghcr.io/devcontainers/features/docker-from-docker:1": {
			"version": "latest"
		},
		// To install the latest Quarto v1.*.
		"ghcr.io/rocker-org/devcontainer-features/quarto-cli:1": {
			"version": "prerelease",
			"installTinyTex": "false",
			"installChromium": "false"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"REditorSupport.r"
			]
		}
	}
}
